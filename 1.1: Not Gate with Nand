/**
 * Not gate:
 * out = not in
 */

CHIP Not {
    IN in;
    OUT out;

    PARTS:
    // Put your code here:
	Nand(a=in,b=in,out=out);
}



// The code we add for not gate in nand values. In the software we downloaded we can load this as chip to test the outputs in the
// Hardware simulator
